<span class="close"><svg><use xlink:href="#shape-arrow-down"></use></svg></span>

{{!-- RESULTS --}}
<ul class="analysis-stats">
  {{#if totalArea}}
    <li>
      <span class="stats-title">Total selected area</span>
      <span class="stats-count"><strong>{{totalArea}}</strong> ha</span>
    </li>
  {{/if}}


  {{!-- LOSS --}}
  {{#ifCond slug "===" 'loss_by_driver' || slug "===" 'loss'}}
    <li>
      <span class="stats-title">Loss <a href="#" class="source" data-source="how_to_adjust_date">{{lossDateRange}}</a><span class="stats-threshold" title="Click the gear icon located on the map to change the tree cover canopy density">with >{{threshold}}% <a href="#" class="canopy-button">canopy density</a></span></span>
      <span class="stats-count" style="color: {{layer.category_color}};"><strong>{{lossAlerts}}</strong> ha</span>
    </li>

    {{#if both}}
      <li>
        <span class="stats-title">Gain 2001-2012</span>
        <span class="stats-count" style="color: #6D6DE5;"><strong>{{gainAlerts}}</strong> ha</span>
      </li>
    {{/if}}

    <li>
      <span class="stats-title">Tree cover (2000)<span class="stats-threshold" title="Click the gear icon located on the map to change the tree cover canopy density">with >{{threshold}}% <a href="#" class="canopy-button">canopy density</a></span></span>
      <span class="stats-count" style="color: #97bd3d;"><strong>{{extent}}</strong> ha</span>
    </li>

    <li>
      <a href="#" class="advanced-analysis-button">View Advanced Analysis</a>
    </li>

    {{#if warning_text}}
      <li>
        <div class="stats-notice">This algorithm approximates the results by sampling the selected area. Results are more accurate at closer zoom levels.</div>
      </li>
    {{/if}}

    {{#if warning_extent_text}}
      <li>
        <div class="stats-notice">NOTE: tree cover loss and gain statistics cannot be compared against each other. <a href="#" class="source" data-source="loss-compare-gain">Learn more</a>.</div>
      </li>
    {{/if}}
  {{/ifCond}}


  {{!-- FORESTGAIN --}}
  {{#ifCond slug "===" 'forestgain'}}
    <li>
      <span class="stats-title">Gain 2001-2012</span>
      <span class="stats-count" style="color: #6D6DE5;"><strong>{{gainAlerts}}</strong> ha</span>
    </li>

    <li>
      <span class="stats-title">Tree cover (2000)<span class="stats-threshold" title="Click the gear icon located on the map to change the tree cover canopy density">with >{{threshold}}% <a href="#" class="canopy-button">canopy density</a></span></span>
      <span class="stats-count" style="color: #97bd3d;"><strong>{{extent}}</strong> ha</span>
    </li>

    {{#if warning_text}}
      <li>
        <div class="stats-notice">This algorithm approximates the results by sampling the selected area. Results are more accurate at closer zoom levels.</div>
      </li>
    {{/if}}

    {{#if warning_extent_text}}
      <li>
        <div class="stats-notice">NOTE: tree cover loss and gain statistics cannot be compared against each other. <a href="#" class="source" data-source="loss-compare-gain">Learn more</a>.</div>
      </li>
    {{/if}}
  {{/ifCond}}


  {{!-- EXTENT --}}
  {{#ifCond slug "===" 'extent'}}
    <li>
      <span class="stats-title">Tree cover (2000)<span class="stats-threshold" title="Click the gear icon located on the map to change the tree cover canopy density">with >{{threshold}}% <a href="#" class="canopy-button">canopy density</a></span></span>
      <span class="stats-count" style="color: #97bd3d;"><strong>{{extent}}</strong> ha</span>
    </li>
  {{/ifCond}}


  {{!-- IMAZON --}}
  {{#ifCond slug "===" 'imazon'}}
    <li>
      <span class="stats-title">Deforestation alerts ({{dateRange}})</span>
      <span class="stats-count" style="color: #F13689;"><strong>{{defor}}</strong> ha</span>
    </li>
    <li>
      <span class="stats-title">Degradation alerts</span>
      <span class="stats-count" style="color: #FA98B9;"><strong>{{degrad}}</strong> ha</span>
    </li>
  {{/ifCond}}


  {{!-- PRODES --}}
  {{#ifCond slug "===" 'prodes'}}
    <li>
      <span class="stats-title">Prodes tree cover loss ({{dateRange}})</span>
      <span class="stats-count" style="color: {{layer.category_color}};"><strong>{{totalAlerts}}</strong> ha</span>
    </li>
  {{/ifCond}}


  {{!-- GUYRA --}}
  {{#ifCond slug "===" 'guyra'}}
    <li>
      <span class="stats-title">Grand Chaco tree cover loss:</span>
      <span class="stats-count" style="color: {{layer.category_color}};"><strong>{{totalAlerts}}</strong> ha</span>
    </li>
  {{/ifCond}}


  {{!-- GLAD --}}
  {{#ifCond slug "===" 'umd_as_it_happens'}}
    <li>
      <span class="stats-title">GLAD Tree Cover Loss Alerts:</span>
      <span class="stats-count" style="color: {{layer.category_color}};"><strong>{{totalAlerts}}</strong></span>
    </li>
  {{/ifCond}}


  {{!-- GLAD PERU --}}
  {{#ifCond slug "===" 'umd_as_it_happens_per'}}
    <li>
      <span class="stats-title">GLAD Tree Cover Loss Alerts:</span>
      <span class="stats-count" style="color: {{layer.category_color}};"><strong>{{totalAlerts}}</strong></span>
    </li>
  {{/ifCond}}


  {{!-- GLAD CONGO --}}
  {{#ifCond slug "===" 'umd_as_it_happens_cog'}}
    <li>
      <span class="stats-title">GLAD Tree Cover Loss Alerts:</span>
      <span class="stats-count" style="color: {{layer.category_color}};"><strong>{{totalAlerts}}</strong></span>
    </li>
  {{/ifCond}}


  {{!-- GLAD INDONESIA --}}
  {{#ifCond slug "===" 'umd_as_it_happens_idn'}}
    <li>
      <span class="stats-title">GLAD Tree Cover Loss Alerts:</span>
      <span class="stats-count" style="color: {{layer.category_color}};"><strong>{{totalAlerts}}</strong></span>
    </li>
  {{/ifCond}}


  {{!-- FORMA --}}
  {{#ifCond slug "===" 'forma'}}
    <li>
      <span class="stats-title">Total alerts ({{dateRange}})</span>
      <span class="stats-count" style="color: {{layer.category_color}};"><strong>{{totalAlerts}}</strong> {{alertsName}}</span>
    </li>
  {{/ifCond}}


  {{!-- FIRES --}}
  {{#ifCond slug "===" 'viirs_fires_alerts'}}
    <li>
      <span class="stats-title">Total alerts ({{dateRange}})</span>
      <span class="stats-count" style="color: {{layer.category_color}};"><strong>{{totalAlerts}}</strong> {{alertsName}}</span>
    </li>
  {{/ifCond}}


  {{!-- TERRAi --}}
  {{#ifCond slug "===" 'terrailoss'}}
    <li>
      <span class="stats-title">Total alerts ({{dateRange}})</span>
      <span class="stats-count" style="color: {{layer.category_color}};"><strong>{{totalAlerts}}</strong> {{alertsName}}</span>
    </li>
  {{/ifCond}}
</ul>


{{!-- FOOTER --}}
<footer class="analysis-footer">
  <button id="analysis-subscribe" class="btn green uppercase analysis-subscribe">Subscribe</button>

  <ul>
    {{#if downloadVisible}}
    <li>
      {{#ifCond slug "===" 'loss'}}
        <a class="analysis-footer-btn download-mobile-link" data-type="forest-change" href="https://earthenginepartners.appspot.com/science-2013-global-forest" target="_blank">
          <svg class="icon"><use xlink:href="#icon-analysis-download"></use></svg>
          <span>Download data</span>
        </a>
        {{else}}
        {{#ifCond slug "===" 'forestgain'}}
          <a class="analysis-footer-btn download-mobile-link" data-type="forest-change" href="https://earthenginepartners.appspot.com/science-2013-global-forest" target="_blank">
            <svg class="icon"><use xlink:href="#icon-analysis-download"></use></svg>
            <span>Download data</span>
          </a>
          {{else}}
          <button class="analysis-footer-btn js-dropdown-btn">
            <div>
              <svg class="icon"><use xlink:href="#icon-analysis-download"></use></svg>
              <svg class="icon -inactive"><use xlink:href="#shape-close"></use></svg>
            </div>
            <span>Download data</span>
            <ul class="analysis-footer-dropdown js-dropdown">
              <li><span>By downloading data you agree to the <a href="/terms">GFW Terms of Service</a></span></li>
              <li><a class="download-mobile-link" data-type="forest-change" href="{{download.svg}}" target="_blank">SVG</a></li>
              <li><a class="download-mobile-link" data-type="forest-change" href="{{download.geojson}}" target="_blank">GeoJSON</a></li>
              <li><a class="download-mobile-link" data-type="forest-change" href="{{download.shp}}" target="_blank">SHP</a></li>
              <li><a class="download-mobile-link" data-type="forest-change" href="{{download.kml}}" target="_blank">KML</a></li>
              <li><a class="download-mobile-link" data-type="forest-change" href="{{download.csv}}" target="_blank">CSV</a></li>
              <li><a class="download-mobile-link" data-type="forest-change" href="http://oneclick.carto.com/?file={{download.cdb}}&provider=gfw'" target="_blank">Open in Carto DB</a></li>
            </ul>
          </button>
        {{/ifCond}}
      {{/ifCond}}
    </li>
    {{/if}}
    <li id="analysis-share">
      <button class="analysis-footer-btn">
        <svg class="icon"><use xlink:href="#icon-analysis-share"></use></svg>
        <span>Share</span>
      </button>
    </li>

    <li id="analysis-delete">
      <button class="analysis-footer-btn">
        <svg class="icon"><use xlink:href="#icon-analysis-remove"></use></svg>
        <span>Discard analysis</span>
      </button>
    </li>
  </ul>
</footer>
