<!DOCTYPE html>
<html>
  <head>
    <%= render 'shared/meta' %>
    <%= render 'shared/favicons' %>

    <%= yield :head %>

    <!-- custom -->
    <link rel="canonical" href="<%= request.original_url %>" />
    <script src="https://cdn.optimizely.com/js/7988050790.js"></script>
    <%= render 'shared/gfw_assets' %>
    <% if controller_name != 'map' %>
    <% end %>
    <%= render 'shared/cache_keys' %>
    <%= render 'shared/track' %>

    <!-- render react app pack -->
    <%= javascript_pack_tag controller_name %>
    <%= stylesheet_pack_tag controller_name %>

  </head>
  <body>
    <div id="headerGfw"></div>
    <%= yield %>
    <div id="footerGfw"></div>
  </body>
</html>
